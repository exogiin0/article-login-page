<% if(locals.errors){ %>
  <% errors.forEach(function(error) { %>
    <p class="error_message"><%= error.msg %></p>
<% }); %> <% } %>

<% if(success_msg != ''){ %>
  <p class="success_message"><%= success_msg %></p>
<% } %>

<% if(error_msg != ''){ %>
  <p class="error_message"><%= error_msg %></p>
<% } %>

<% if(error != ''){ %>
  <p class="error_message"><%= error %></p>
<% } %>



